[
    {
        "id": "da_ch7_1",
        "text": "What is Factor Analysis?",
        "options": [
            {
                "id": "a",
                "text": "Analyzing factory output."
            },
            {
                "id": "b",
                "text": "A statistical method used to describe variability among observed variables in terms of fewer unobserved variables (factors)."
            },
            {
                "id": "c",
                "text": "Classification."
            },
            {
                "id": "d",
                "text": "Regression."
            }
        ],
        "correctOptionId": "b",
        "hint": "Dimension reduction.",
        "explanation": "Factor analysis reduces a large number of variables into fewer numbers of factors."
    },
    {

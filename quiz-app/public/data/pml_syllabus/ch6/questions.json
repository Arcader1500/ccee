            },
            {
                "id": "b",
                "text": "Walking without a goal."
            },
            {
                "id": "c",
                "text": "A stationary series."
            },
            {
                "id": "d",
                "text": "Binary data."
            }
        ],
        "correctOptionId": "a",
        "hint": "Non-stationary trend.",
        "explanation": "Often used to model stock prices; best forecast is today's price."
    },

[
    {
        "id": "pml_ch6_1",
        "text": "What is 'Time Series' data?",
        "options": [
            {
                "id": "a",
                "text": "Data collected at regular intervals over time (e.g., daily stock price)."
            },
            {
                "id": "b",
                "text": "A list of times only."
            },
            {
                "id": "c",
                "text": "Random data points."
            },
            {
                "id": "d",
                "text": "Data without labels."
            }
        ],
        "correctOptionId": "a",
        "hint": "Temporal sequence.",
        "explanation": "Order matters in time series; you cannot shuffle the data."
    },
    {
        "id": "pml_ch6_2",
        "text": "What is 'Stationarity' in time series?",
        "options": [
            {
                "id": "a",
                "text": "When statistical properties (mean, variance) do not change over time."
            },
            {
                "id": "b",
                "text": "When data is standing still."
            },
            {
                "id": "c",
                "text": "When the file stays on disk."
            },
            {
                "id": "d",
                "text": "Predicting the future."
            }
        ],
        "correctOptionId": "a",
        "hint": "Consistent statistics.",
        "explanation": "Many models (like ARIMA) assume or require the data to be stationary."
    },
    {
        "id": "pml_ch6_3",
        "text": "What is 'Trend' in time series?",
        "options": [
            {
                "id": "a",
                "text": "Long-term increase or decrease in the data."
            },
            {
                "id": "b",
                "text": "Weekly patterns."
            },
            {
                "id": "c",
                "text": "Random noise."
            },
            {
                "id": "d",
                "text": "Total count."
            }
        ],
        "correctOptionId": "a",
        "hint": "Overall direction.",
        "explanation": "Captures the general movement (e.g., global temperature rising)."
    },
    {
        "id": "pml_ch6_4",
        "text": "What is 'Seasonality'?",
        "options": [
            {
                "id": "a",
                "text": "Patterns that repeat at a regular fixed period (e.g., high ice cream sales in summer)."
            },
            {
                "id": "b",
                "text": "Moving to a new city."
            },
            {
                "id": "c",
                "text": "A random event."
            },
            {
                "id": "d",
                "text": "Long term trend."
            }
        ],
        "correctOptionId": "a",
        "hint": "Recurring patterns.",
        "explanation": "Repeats every day, week, month, or year."
    },
    {
        "id": "pml_ch6_5",
        "text": "What is 'Autocorrelation' (ACF)?",
        "options": [
            {
                "id": "a",
                "text": "Relationship between a variable's current value and its past values."
            },
            {
                "id": "b",
                "text": "Matching cars automatically."
            },
            {
                "id": "c",
                "text": "Correlation with another feature."
            },
            {
                "id": "d",
                "text": "Total sum."
            }
        ],
        "correctOptionId": "a",
        "hint": "Correlation with self.",
        "explanation": "Measures how today's value depends on yesterday's or last year's."
    },
    {
        "id": "pml_ch6_6",
        "text": "What does a 'Moving Average' (MA) do?",
        "options": [
            {
                "id": "a",
                "text": "Smooths out short-term fluctuations to highlight longer-term trends."
            },
            {
                "id": "b",
                "text": "Calculates speed."
            },
            {
                "id": "c",
                "text": "Finds the peak."
            },
            {
                "id": "d",
                "text": "Deletes data."
            }
        ],
        "correctOptionId": "a",
        "hint": "Smoothing tool.",
        "explanation": "Calculates average of the last 'n' observations."
    },
    {
        "id": "pml_ch6_7",
        "text": "What is 'ARIMA'?",
        "options": [
            {
                "id": "a",
                "text": "AutoRegressive Integrated Moving Average."
            },
            {
                "id": "b",
                "text": "Audio Recorded Information Machine."
            },
            {
                "id": "c",
                "text": "Average Rate In Metric Area."
            },
            {
                "id": "d",
                "text": "None."
            }
        ],
        "correctOptionId": "a",
        "hint": "Classic time series model.",
        "explanation": "Widely used model for forecasting stationary or differenced data."
    },
    {
        "id": "pml_ch6_8",
        "text": "In ARIMA(p, d, q), what does 'p' stand for?",
        "options": [
            {
                "id": "a",
                "text": "Auto-regressive part (number of lag observations)."
            },
            {
                "id": "b",
                "text": "Degree of differencing."
            },
            {
                "id": "c",
                "text": "Moving average window size."
            },
            {
                "id": "d",
                "text": "Price."
            }
        ],
        "correctOptionId": "a",
        "hint": "AR part.",
        "explanation": "Describes how much the current value depends on past values."
    },
    {
        "id": "pml_ch6_9",
        "text": "In ARIMA(p, d, q), what does 'd' stand for?",
        "options": [
            {
                "id": "a",
                "text": "Degree of differencing needed to make data stationary."
            },
            {
                "id": "b",
                "text": "Depth of tree."
            },
            {
                "id": "c",
                "text": "Date."
            },
            {
                "id": "d",
                "text": "Difference of means."
            }
        ],
        "correctOptionId": "a",
        "hint": "Stationarity adjustment.",
        "explanation": "Number of times the raw observations are subtracted from their preceding values."
    },
    {
        "id": "pml_ch6_10",
        "text": "In ARIMA(p, d, q), what does 'q' stand for?",
        "options": [
            {
                "id": "a",
                "text": "Order of the moving average (error part)."
            },
            {
                "id": "b",
                "text": "Quality of data."
            },
            {
                "id": "c",
                "text": "Quantity."
            },
            {
                "id": "d",
                "text": "Queue size."
            }
        ],
        "correctOptionId": "a",
        "hint": "MA part.",
        "explanation": "Describes how much the current value depends on past forecast errors."
    },
    {
        "id": "pml_ch6_11",
        "text": "What is 'Exponential Smoothing'?",
        "options": [
            {
                "id": "a",
                "text": "A rule of thumb where past observations are weighted with exponentially decreasing weights."
            },
            {
                "id": "b",
                "text": "Smoothing using a power plant."
            },
            {
                "id": "c",
                "text": "Linear average."
            },
            {
                "id": "d",
                "text": "Deleting zeros."
            }
        ],
        "correctOptionId": "a",
        "hint": "Recent data is more important.",
        "explanation": "Calculates forecast based on weighted average of past data."
    },
    {
        "id": "pml_ch6_12",
        "text": "What is 'Holt-Winters' method?",
        "options": [
            {
                "id": "a",
                "text": "Triple exponential smoothing that handles Trend AND Seasonality."
            },
            {
                "id": "b",
                "text": "A cold weather algorithm."
            },
            {
                "id": "c",
                "text": "Simple average."
            },
            {
                "id": "d",
                "text": "Regression on time."
            }
        ],
        "correctOptionId": "a",
        "hint": "Trend + Seasonality smoothing.",
        "explanation": "Uses level, trend, and seasonal components to forecast."
    },
    {
        "id": "pml_ch6_13",
        "text": "What is 'Lag' in time series?",
        "options": [
            {
                "id": "a",
                "text": "A previous time step (e.g., Lag 1 is yesterday's value)."
            },
            {
                "id": "b",
                "text": "Slow computer performance."
            },
            {
                "id": "c",
                "text": "The end of the data."
            },
            {
                "id": "d",
                "text": "None."
            }
        ],
        "correctOptionId": "a",
        "hint": "Past value.",
        "explanation": "Converting 'time' into features often involves creating lag columns."
    },
    {
        "id": "pml_ch6_14",
        "text": "What is 'Differencing'?",
        "options": [
            {
                "id": "a",
                "text": "Subtracting current value from previous value to remove trend/seasonality."
            },
            {
                "id": "b",
                "text": "Comparing two datasets."
            },
            {
                "id": "c",
                "text": "Finding error."
            },
            {
                "id": "d",
                "text": "Subtraction only."
            }
        ],
        "correctOptionId": "a",
        "hint": "Making data stationary.",
        "explanation": "y'_t = y_t - y_{t-1}."
    },
    {
        "id": "pml_ch6_15",
        "text": "A 'White Noise' series has?",
        "options": [
            {
                "id": "a",
                "text": "Mean 0, constant variance, and NO autocorrelation."
            },
            {
                "id": "b",
                "text": "Clear trend."
            },
            {
                "id": "c",
                "text": "Repeating seasonality."
            },
            {
                "id": "d",
                "text": "All values are white."
            }
        ],
        "correctOptionId": "a",
        "hint": "Random signal.",
        "explanation": "It's purely random and cannot be used for forecasting."
    },
    {
        "id": "pml_ch6_16",
        "text": "What is 'PACF' (Partial Autocorrelation Function)?",
        "options": [
            {
                "id": "a",
                "text": "Correlation between variable and its lag after removing effects of smaller lags."
            },
            {
                "id": "b",
                "text": "Part of an audio file."
            },
            {
                "id": "c",
                "text": "Random correlation."
            },
            {
                "id": "d",
                "text": "Total correlation."
            }
        ],
        "correctOptionId": "a",
        "hint": "Direct influence of lag.",
        "explanation": "Used to find the order 'p' for an AR model."
    },
    {

    },
    {
        "id": "pml_ch5_16",
        "text": "What is 'CatBoost' optimized for?",
        "options": [
            {
                "id": "a",
                "text": "Handling Categorical features automatically."
            },
            {
                "id": "b",
                "text": "Classifying cats."
            },
            {
                "id": "c",
                "text": "Boosting for slow laptops."
            },
            {
                "id": "d",
                "text": "Speed only."
            }
        ],
        "correctOptionId": "a",

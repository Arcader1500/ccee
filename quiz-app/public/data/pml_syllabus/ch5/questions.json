    },
    {
        "id": "pml_ch5_23",
        "text": "Which method is better if base models are very different?",
        "options": [
            {
                "id": "a",
                "text": "Stacking."
            },
            {
                "id": "b",
                "text": "Bagging."
            },
            {
                "id": "c",
                "text": "Copying."
            },
            {
                "id": "d",
                "text": "Sorting."
            }
        ],
        "correctOptionId": "a",
